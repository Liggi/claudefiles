{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "900000",
    "BASH_MAX_TIMEOUT_MS": "900000"
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/jasonliggi/.claude/hooks/file-tools-hook.sh"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/jasonliggi/.claude/hooks/comment_analyzer.py"
          }
        ]
      },
      {
        "matcher": "MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/jasonliggi/.claude/hooks/comment_analyzer.py"
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/jasonliggi/.claude/hooks/comment_analyzer.py"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash /Users/jasonliggi/.claude/hooks/statusline-command.sh"
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754038067914
  },
  "toolPermissions": {
    "allow": [
      "Bash(echo * > ~/.claude/note)",
      "Bash(rm ~/.claude/note)"
    ],
    "deny": []
  },
  "model": "opusplan",
  "includeCoAuthoredBy": false,
  "gitAttribution": false
